<h1>Grand Decision</h1>

<table class="zebra-striped">
	<thead>
		<tr>
			<th>Name</th>
			<th>Table</th>
			<th>Choice</th>
		</tr>
	</thead>
	<tbody>
	<% @residents.each do |r| %>
		<tr>
			<td><%= r.name %></td>
			<td><%= r.choice %></td>
			<td><%= r.choice_num %></td>
		</tr>
	<% end %>
	</tbody>
</table>